/* ..Sonali */

#top_div
{
	background-color:#b0c4de;
	background-image:url('samples.png');
	margin: 0; padding: 0; 
	list-style-type: none; list-style-image: none;
}

#footer_div
{
	text-align:center;
	background-color:#b0c4de;
	position:absolute;bottom:0; left: 0;right:0;	
}

#wrap
{
	background-color:#e0ffff;
	
}
body
{
	
	background-color:#e0ffff;
	
}

ul
{
		background-color:#b0c4de;
		font-weight:bold;
}

ul a:hover {
color: #FFF;
}

li
{
	float:left;
	background-color:#b0c4de;
	display: inline; 
}

ul a{
background-repeat: no-repeat;
background-position: right;
padding-right: 22px;
padding-left: 22px;
display: block;
line-height: 50px;
text-decoration: none;
font-family: Georgia, "Times New Roman", Times, serif;
font-size: 20px;

}
fieldset
{
	background-color:#b0c4de;

}
